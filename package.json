{
	"name": "svg-render",
	"version": "0.0.1",
	"description": "small library for drawing svg path",
	"license": "MIT",
	"main": "src/index.js",
	"keywords": [
		"svg",
		"javascript"
	],
	"author": {
		"name": "Amit Leshem",
		"url": "http://www.happy-spider.com/"
	},
	"homepage": "https://github.com/amit-82/SVGRender",
	"repository": {
		"type": "git",
		"url": "https://github.com/amit-82/SVGRender.git"
	},
	"scripts": {
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build": "webpack --config webpack.config.js",
		"build:watch": "webpack --config webpack.config.js --watch && nodemon --watch src/**.* --ignore *.test.ts",
		"example:build": "webpack --config webpack-examples.config.js",
		"example:watch": "webpack --config webpack-examples.config.js --watch && nodemon --watch example/src/**.* --exec npm run example:build",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"dependencies": {
		"enhanced-resolve": "^4.1.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-typescript": "^7.9.0",
		"@types/jest": "^25.2.1",
		"@types/node": "^13.11.1",
		"babel-cli": "^6.26.0",
		"babel-loader": "^8.1.0",
		"babel-preset-env": "^1.7.0",
		"html-webpack-plugin": "^4.0.4",
		"jest": "^25.3.0",
		"nodemon": "^2.0.3",
		"remove-files-webpack-plugin": "^1.4.0",
		"ts-jest": "^25.3.1",
		"ts-loader": "^7.0.0",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	}
}
